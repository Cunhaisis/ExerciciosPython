from datetime import datetime

x = int(input('Qual o ano de nascimento ? '))

now = datetime.now()
idade  = now.year - x

if idade < 9:
    print('MIRIM')
elif idade > 9 and idade < 14:
    print('INFANTIL')
elif idade > 14 and idade < 19:
    print('JUNIOR')
elif idade > 19 and idade < 20:
    print('SENIOR')
elif idade > 20:
    print('MASTER')
else:
    print('SEM CLASSIFICAÇÃO')
